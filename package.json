    "eslint-plugin-react-hooks": "4.6.0",
    "jest": "29.7.0",
    "rss-parser": "3.12.0",
    "prettier": "3.2.5",
    "react": "18.2.0",
    "react-native": "0.73.0",
    "ts-jest": "29.1.1",
